<%- include('includes/head.ejs') %>
<link rel="stylesheet" href="/stylesheets/login.css">
</head>
<body>
    <!-- <%- include('includes/navigation.ejs') %> -->
    <div class="limiter">
        <div class="container-login100" style="background-image: url('images/login-banner.jpg');">
            <div class="wrap-login100 p-t-30 p-b-50">
                <span class="login100-form-title p-b-41">
                    Login
                </span>
                <!-- <p class="errorMessage">Please Enter Correct Credentials!</p> -->
                <div class="login100-form">
                    <div class="validate-form p-b-33 p-t-5 rounded-circle" style="background-color: white;">

                        <div class="wrap-input100 validate-input" data-validate="Enter Correct username">
                            <input class="input100 username" type="text" name="username" placeholder="User name" >
                            <span class="focus-input100" data-placeholder="&#xe82a;"></span>
                        </div>
                        <div class="wrap-input100 validate-input" data-validate="Enter Correct password">
                            <input class="input100 password" type="password" name="password" maxlength="8" placeholder="Password" >
                            <span class="focus-input100" data-placeholder="&#xe80f;"></span>
                        </div>
                        <div class="container-signup m-t-20">
                            <span class="container-signup-label m-l-8">Don't have Account? </span><a class="container-signup-link" href="/signup">Signup here</a>
                        </div>
                        <div class="container-login100-form-btn m-t-15 p-b-16">
                            <button class="login100-form-btn">
                                Login
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="/javascripts/login.js"></script>
    <%- include('includes/end.ejs') %>